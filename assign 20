def prime(n):
       c=0
       for i in range(1,n+1):
                if n%i==0: 
                    c=c+1
       return c
n=int(input('enter n value:'))
l=list(map(int,input("enter values:").split()))
l2=[]
for i in l:
      if prime(i)==2:
            l2.append(i)
print(l2)
print('max prime=',max(l2))
print('min prime=',min(l2))
